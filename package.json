{
  "name": "projectryan",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.3.2",
    "@material-ui/icons": "^3.0.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "typeface-roboto": "0.0.54"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "flow": "flow",
    "format": "prettier-eslint --parser --write \"{pages/**/*.js,components/**/*.js,modules/**/*.js,core/**/*.js,hocs/**/*.js,src/**/*.js}\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "flow && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier-eslint --parser --write \"{pages/**/*.js,components/**/*.js,modules/**/*.js,core/**/*.js,hocs/**/*.js,src/**/*.js}\"",
      "eslint .",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint": "^5.6.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "flow-bin": "^0.85.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.0.4",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1"
  }
}
